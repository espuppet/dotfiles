if has("gui_running")
    "colorscheme solarized
    colorscheme molokai
else
    colorscheme molokai
    let g:rehash256=1
    let g:molokai_original=1
endif

"let g:ycm_global_ycm_extra_conf = '~/.ycm_extra_conf.py'

let g:Vimim_cloud = 'baidu,qq'
