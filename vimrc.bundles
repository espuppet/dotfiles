let g:solarized_termcolors=256

nmap <leader>f :NERDTreeToggle<CR>
let g:NERDTreeChDirMode=2

nmap <leader>b :CtrlPBuffer<CR>
let g:ctrlp_extensions = ['tag']

let g:SuperTabDefaultCompletionType = "context"
